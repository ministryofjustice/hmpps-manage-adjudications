{% extends "../partials/layout.njk" %}

{% set title = "Search for a prisoner to start a new report" %}

{% block beforeContent %}
    {{ breadcrumb() }}
{% endblock %}

{% block content %}
    {% if errors | length %}
        {{ govukErrorSummary({
            titleText: "There is a problem",
            errorList: errors,
            attributes: { "data-qa": "error-summary" }
        }) }}
    {% endif %}

    <h1 class="govuk-heading-l">{{ title }}</h1>

{% endblock %}